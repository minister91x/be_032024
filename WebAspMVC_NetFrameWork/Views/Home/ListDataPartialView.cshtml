@model List<DataAccess.NetFrameWork.DTO.Post>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>

        @if (Model.Count > 0)
        {
            for (int i = 0; i < Model.Count; i++)
            {
                var item = Model[i];
                <tr>
                    <td>@item.PostID</td>
                    <td>@item.PostName</td>
                </tr>
            }
        }
    </tbody>
</table>
