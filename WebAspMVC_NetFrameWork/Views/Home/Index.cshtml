@{
    ViewBag.Title = "Home Page";
}
@model List<WebAspMVC_NetFrameWork.Models.DemoModels>

<div class="jumbotron">

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>

            @if (Model.Count > 0)
            {
                for (int i = 0; i < Model.Count; i++)
                {
                    var item = Model[i];
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
